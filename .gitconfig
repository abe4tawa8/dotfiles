[user]
	name = Yoichi Isozaki
	email = abe4tawa8@gmail.com
[core]
	editor = /usr/bin/vim
	excludesfile = ~/.gitignore_global
[alias]
	co = checkout
	ci = commit
	st = status
	di = diff
	ds = diff --staged
	sg = log --graph --abbrev-commit --pretty=oneline
	br = branch
[merge]
	ff = false
[pull]
	rebase = true
[push]
	default = upstream
[color]
	ui = auto
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
